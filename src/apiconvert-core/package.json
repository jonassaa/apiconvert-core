{
  "name": "@apiconvert/core",
  "version": "0.0.18",
  "description": "Apiconvert core contracts and schemas for conversion rules.",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonassaa/apiconvert-core"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apiconvert": "./bin/apiconvert.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "bin",
    "dist",
    "schemas",
    "README.md"
  ],
  "scripts": {
    "sync-schemas": "node ./scripts/sync-schemas.mjs",
    "build": "npm run sync-schemas && tsc -p tsconfig.json"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.7"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
