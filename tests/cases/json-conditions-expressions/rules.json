      "outputPaths": [
        "existsWord"
      ],
      "source": {
        "type": "condition",
        "expression": "exists(path(name))",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "eqSymbol"
      ],
      "source": {
