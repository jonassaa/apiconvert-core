        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "ltAlias"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) lt 6",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "lteAlias"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) lte 5",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "andWord"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) gt 4 and path(count) lt 10",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "orWord"
      ],
      "source": {
        "type": "condition",
        "expression": "path(role) == 'admin' or path(role) == 'guest'",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "andSymbol"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) > 4 && path(count) < 10",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "orSymbol"
      ],
      "source": {
        "type": "condition",
        "expression": "path(role) == 'admin' || path(role) == 'guest'",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
