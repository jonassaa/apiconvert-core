        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "rootPath"
      ],
      "source": {
        "type": "condition",
        "expression": "path($.name) == 'Ada'",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "missingPathNull"
      ],
      "source": {
        "type": "condition",
