{
  "inputFormat": "json",
  "outputFormat": "json",
  "rules": [
    {
      "kind": "array",
      "inputPath": "items",
      "outputPaths": [
        "lines",
        "copies.lines"
      ],
      "itemRules": [
        {
          "kind": "field",
          "outputPaths": [
            "code",
            "identifiers.sku"
          ],
          "source": {
            "type": "path",
            "path": "sku"
          }
        },
        {
          "kind": "field",
          "outputPaths": [
            "quantity",
            "metrics.qty"
          ],
          "source": {
            "type": "path",
            "path": "qty"
          }
        }
      ]
    }
  ]
}
