{
  "version": 2,
  "inputFormat": "query",
  "outputFormat": "json",
  "rules": [
    {
      "kind": "field",
      "outputPaths": [
        "name"
      ],
      "source": {
        "type": "path",
        "path": "user.name"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "active"
      ],
      "source": {
        "type": "transform",
        "path": "user.active",
        "transform": "boolean"
      }
    }
  ]
}
