{
  "inputFormat": "json",
  "outputFormat": "json",
  "rules": [
    {
      "kind": "field",
      "outputPaths": [
        "exists"
      ],
      "source": {
        "type": "condition",
        "expression": "exists(path(flag))",
        "trueValue": "yes",
        "falseValue": "no"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "equals"
      ],
      "source": {
        "type": "condition",
        "expression": "path(flag) == 'yes'",
        "trueValue": "match",
        "falseValue": "no"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "includes"
      ],
      "source": {
        "type": "condition",
        "expression": "path(tags.1) == 'b'",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "gt"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) > 3",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "lt"
      ],
      "source": {
        "type": "condition",
        "expression": "path(count) < 10",
        "trueValue": "y",
        "falseValue": "n"
      }
    }
  ]
}
