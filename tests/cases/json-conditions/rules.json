{
  "version": 2,
  "inputFormat": "json",
  "outputFormat": "json",
  "fieldMappings": [
    {
      "outputPath": "exists",
      "source": {
        "type": "condition",
        "expression": "exists(path(flag))",
        "trueValue": "yes",
        "falseValue": "no"
      }
    },
    {
      "outputPath": "equals",
      "source": {
        "type": "condition",
        "expression": "path(flag) == 'yes'",
        "trueValue": "match",
        "falseValue": "no"
      }
    },
    {
      "outputPath": "includes",
      "source": {
        "type": "condition",
        "expression": "path(tags.1) == 'b'",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "outputPath": "gt",
      "source": {
        "type": "condition",
        "expression": "path(count) > 3",
        "trueValue": "y",
        "falseValue": "n"
      }
    },
    {
      "outputPath": "lt",
      "source": {
        "type": "condition",
        "expression": "path(count) < 10",
        "trueValue": "y",
        "falseValue": "n"
      }
    }
  ],
  "arrayMappings": []
}
