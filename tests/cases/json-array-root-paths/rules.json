{
  "version": 2,
  "inputFormat": "json",
  "outputFormat": "json",
  "rules": [
    {
      "kind": "array",
      "inputPath": "$.items",
      "outputPaths": [
        "$.lines"
      ],
      "itemRules": [
        {
          "kind": "field",
          "outputPaths": [
            "code"
          ],
          "source": {
            "type": "path",
            "path": "sku"
          }
        },
        {
          "kind": "field",
          "outputPaths": [
            "quantity"
          ],
          "source": {
            "type": "path",
            "path": "qty"
          }
        },
        {
          "kind": "field",
          "outputPaths": [
            "tenant"
          ],
          "source": {
            "type": "path",
            "path": "$.meta.tenant"
          }
        }
      ]
    }
  ]
}
