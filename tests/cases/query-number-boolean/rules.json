{
  "version": 2,
  "inputFormat": "query",
  "outputFormat": "json",
  "rules": [
    {
      "kind": "field",
      "outputPaths": [
        "count"
      ],
      "source": {
        "type": "transform",
        "path": "count",
        "transform": "number"
      }
    },
    {
      "kind": "field",
      "outputPaths": [
        "active"
      ],
      "source": {
        "type": "transform",
        "path": "active",
        "transform": "boolean"
      }
    }
  ]
}
